<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentreportcard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentReportCard">
        <activity
            android:label="Denver's Report Card"
            android:name=".Denver"
            android:exported="false" />
        <activity
            android:label="Brilin's Report Card"
            android:name=".Brillin"
            android:exported="false" />
        <activity
            android:label="Nairobi's Report Card"
            android:name=".Nairobi"
            android:exported="false" />
        <activity
            android:label="Rio's Report Card"
            android:name=".Rio"
            android:exported="false" />
        <activity
            android:label="Helsinki's Report Card"
            android:name=".Helsinki"
            android:exported="false" />
        <activity
            android:label="Proffeser's Report Card"
            android:name=".Proffeser"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>